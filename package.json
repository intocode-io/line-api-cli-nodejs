{
  "name": "line-api-cli",
  "version": "1.3.8",
  "description": "LINE API CLIs for Node.js",
  "main": "lib/index.js",
  "bin": {
    "line": "bin/cli/line-cli.js",
    "linetv": "bin/cli/linetv-cli.js",
    "richmenu": "bin/cli/richmenu-cli.js",
    "liff": "bin/cli/liff-cli.js",
    "things": "bin/cli/things-cli.js"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run chmod:clis",
    "chmod:clis": "chmod +x bin/cli/*-cli.js",
    "clean": "rimraf bin",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "postinstall": "node bin/postinstall.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "line": "ts-node lib/cli/line-cli.ts",
    "generate:version": "cross-var \"echo \\\"export const version = '%npm_package_version%';\\\" > lib/version.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micksatana/line-api-cli-nodejs.git"
  },
  "keywords": [
    "LINE API",
    "Messaging API",
    "LINE Things",
    "Chat Bot",
    "Command Line Interface"
  ],
  "author": "Satana Charuwichitratana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micksatana/line-api-cli-nodejs/issues"
  },
  "homepage": "https://github.com/micksatana/line-api-cli-nodejs#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.1",
    "console-table": "^0.1.2",
    "console.table": "^0.10.0",
    "js-yaml": "^4.0.0",
    "prompts": "^2.4.0",
    "qs": "^6.10.1",
    "ramda": "^0.27.1",
    "terminal-kit": "^2.0.7"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/jest": "^26.0.21",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.37",
    "@types/prompts": "^2.0.9",
    "@types/ramda": "^0.27.39",
    "@types/terminal-kit": "^1.28.2",
    "cross-var": "^1.1.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-extended": "^0.11.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run generate:version && git add lib/version.ts && npm run build"
    }
  }
}
